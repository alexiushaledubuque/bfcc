{{# each products }}
	    <div class="row">
	      {{# each this }}
	          <div class="col-md-4">
	          	<a href="/add-to-cart/{{this._id}}" class="btn btn-default pull-right minus-btn" role="button">
	            	<span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
	            </a>
	              <div class="thumbnail">
	                  <img src="{{ this.imagePath }}" alt="..." class="img-responsive">
	                  <div class="caption">
	                      <h3>{{ this.title }}</h3>
	                      <p class="description">{{ this.description }}</p>
	                      <div class="clearfix">
	                         <div class="price pull-left">${{ this.price }}</div>
	                         <div class="quantity pull-left">
										          <input id="bg-quanity" type="text" name="name" value="0" readonly="">
										        </div>

	                          <a href="/add-to-cart/{{this._id}}" class="btn btn-success pull-right" role="button">
	                          	<span class="glyphicon glyphicon-shopping-cart" aria-hidden="true">
	                          	</span>  Add to Cart
	                          </a>
	                      </div>
	                  </div>
	              </div>
	          </div>
	      {{/each}}
	    </div>
		{{/each}}
